{
  "compilerOptions": {
    "target": "es5",
    "module": "umd",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "dist",
    "sourceMap": true
  },
  "files": [
    "src/export.ts"
  ],
  "fasdilesGlob": [
    "src/*.ts",

    "!node_modules/**/*"
  ],
  "exclude": [
    "node_modules",
    "typings/index.d.ts"
  ],
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
